{
  "name": "Git Ignore",
  "description": "Template to configure Git Ignore file",
  "type": "module",
  "always_apply": true,
  "tags": [
    "git",
    "ignore",
    "default"
  ],
  "setting_subscription": [
    "use_litmus"
  ],
  "publish_settings": {
    "gitignore_paths": null
  },
  "default_settings": {
    "paths" : [
      ".git/",
      ".*.sw[op]",
      ".metadata",
      ".yardoc",
      ".yardwarns",
      "*.iml",
      "/.bundle/",
      "/.idea/",
      "/.vagrant/",
      "/coverage/",
      "/bin/",
      "/doc/",
      "/Gemfile.local",
      "/Gemfile.lock",
      "/junit/",
      "/log/",
      "/pkg/",
      "/spec/fixtures/manifests/",
      "/spec/fixtures/modules/",
      "/tmp/",
      "/vendor/",
      "/convert_report.txt",
      "/update_report.txt",
      ".DS_Store",
      ".project",
      ".envrc"
    ]
  }
}
